{
    "homepage": "https://ariang.mayswind.net/",
    "description": "AriaNg, a modern web frontend making aria2 easier to use.",
    "license": "MIT",
    "version": "1.1.5",
    "architecture": {
        "64bit": {
            "url": "https://github.com/mayswind/AriaNg/releases/download/1.1.5/AriaNg-1.1.5-AllInOne.zip",
            "extract_to": "AriaNg-1.1.5-AllInOne"
        }
    },
    "checkver": {
        "github": "https://github.com/mayswind/AriaNg",
        "re": "/(?:[\\d.]+)/AriaNg-(?<version>[\\d.]+)-AllInOne.zip",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/mayswind/AriaNg/releases/download/$matchVersion/AriaNg-$matchVersion-AllInOne.zip",
                "extract_dir": "AriaNg-$matchVersion-AllInOne"
            }
        }
    }
}
